  <div id="subheader">
    <div class="container" style="width: 1000px;">
      <div class="container-inside">
        <ul class="links secondary-links">
          <li class="menu-147 first"><%= link_to 'Registrate!', new_register_path %></li>
          <li class="menu-148"><a href="http://ohio.ubuntu-us.org/calendar" title="Calendar">Calendario</a></li>
          <li class="menu-149"><%= link_to '¿Dónde?', donde_path %></li>
          <li class="menu-161 last"><a href="http://ohio.ubuntu-us.org/burningcircle" title="Burning Circle Podcast">Contactanos</a></li>
        </ul>
      </div>
    </div>
  </div>
</div>
<!-- Content -->
<div id="content">
  <div class="container" style="width: 1000px;">
    <div id="sidebar-right" class="sidebar">
      <div id="block-aggregator-category-1" class="block block-aggregator">
