  <%= formtastic_stylesheet_link_tag %>
  <%= stylesheet_link_tag :flutie, 'screen', :media => 'all' %>
  <%= javascript_include_tag "jquery", "jquery-ui", "prefilled_input", "rails", "application" %>
  <%= csrf_meta_tag %>
  <!-- Ubuntu's template data-->
  <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
  <%= stylesheet_link_tag 'ubuntu_template' %>
  <!-- Custom -->
  <%= yield(:head) %>
  <title><%= h(yield(:title) || "UbuCon 2010 - Argentina") %></title>
</head>
<body class="<%= body_class %>">
<!-- Header -->
  <div id="header">
    <div class="container" style="width: 1000px;">
      <ul class="links">
        <li class="menu-137 active-trail first active"><a href="http://ohio.ubuntu-us.org/" title="" class="active">Home</a></li>
        <li class="menu-119"><a href="http://ohio.ubuntuforums.org/" title="">Para quienes?</a></li>
        <li class="menu-134"><a href="https://www.ubuntu-ar.org" title="">Organizado por</a></li>
        <li class="menu-135"><a href="https://wiki.ubuntu.com/ArgentinaTeam" title="">Wiki</a></li>
        <li class="menu-136 last"><a href="http://ubuntu.com/" title="">Ubuntu.com</a></li>
        <li><a href="#">mas cosas aquí</a></li>
      </ul>
      <div id="logo">
        <a href="http://www.ubuntu-ar.org/" title="Ubuntu Argentina">
          <%= image_tag 'logo-ar.png' %>
        </a>
      </div>
      <div id="block-block-3" class="block block-block">
        <div class="content"></div>
      </div>
    </div>
  </div>
